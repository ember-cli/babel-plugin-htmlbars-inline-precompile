{
  "name": "babel-plugin-htmlbars-inline-precompile",
  "version": "2.0.0",
  "description": "Babel plugin to replace tagged template strings with precompiled HTMLBars templates",
  "repository": "https://github.com/ember-cli/babel-plugin-htmlbars-inline-precompile",
  "license": "MIT",
  "author": "Clemens MÃ¼ller <cmueller.418@gmail.com>",
  "scripts": {
    "lint": "eslint --cache .",
    "test": "jest"
  },
  "jest": {
    "projects": [
      "<rootDir>/jest-test.config.js"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-modules-amd": "^7.5.0",
    "@babel/plugin-transform-template-literals": "^7.4.4",
    "common-tags": "^1.8.0",
    "ember-source": "^3.12.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "release-it": "^12.2.1",
    "release-it-lerna-changelog": "^1.0.3"
  },
  "engines": {
    "node": "8.* || 10.* || >= 12.*"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  }
}
